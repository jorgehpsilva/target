// Definindo os valores de faturamento por estado
const faturamento = {
    SP: 67836.43,
    RJ: 36678.66,
    MG: 29229.88,
    ES: 27165.48,
    Outros: 19849.53
};

// Calculando o faturamento total
let totalFaturamento = 0;
for (let estado in faturamento) {
    totalFaturamento += faturamento[estado];
}

// Calculando o percentual de representação de cada estado
for (let estado in faturamento) {
    let percentual = (faturamento[estado] / totalFaturamento) * 100;
    console.log(`O percentual de representação de ${estado} é: ${percentual.toFixed(2)}%`);
}
